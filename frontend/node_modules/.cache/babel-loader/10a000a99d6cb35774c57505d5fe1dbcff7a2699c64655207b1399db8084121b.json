{"ast":null,"code":"import api from '../services/api';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nexport default {\n  name: 'LoginPage',\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const user = {\n      email: '',\n      password: ''\n    };\n    const error = '';\n    const handleLogin = async () => {\n      try {\n        const response = await api.post('/users/login', user);\n        store.commit('setToken', response.data.token);\n        router.push('/');\n      } catch (err) {\n        this.error = 'Login failed.';\n      }\n    };\n    return {\n      user,\n      error,\n      handleLogin\n    };\n  }\n};","map":{"version":3,"names":["api","useStore","useRouter","name","setup","store","router","user","email","password","error","handleLogin","response","post","commit","data","token","push","err"],"sources":["/home/ubuntu/FastApiProject2/frontend/src/views/LoginPage.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Login</h2>\n    <form @submit.prevent=\"handleLogin\">\n      <input v-model=\"user.email\" placeholder=\"Email\" required>\n      <input type=\"password\" v-model=\"user.password\" placeholder=\"Password\" required>\n      <button type=\"submit\">Login</button>\n    </form>\n    <p v-if=\"error\" style=\"color:red\">{{ error }}</p>\n  </div>\n</template>\n\n<script>\nimport api from '../services/api'\nimport { useStore } from 'vuex'\nimport { useRouter } from 'vue-router'\n\nexport default {\n  name: 'LoginPage',\n  setup() {\n    const store = useStore()\n    const router = useRouter()\n    const user = { email: '', password: '' }\n    const error = ''\n\n    const handleLogin = async () => {\n      try {\n        const response = await api.post('/users/login', user)\n        store.commit('setToken', response.data.token)\n        router.push('/')\n      } catch (err) {\n        this.error = 'Login failed.'\n      }\n    }\n\n    return { user, error, handleLogin }\n  }\n}\n</script>\n"],"mappings":"AAaA,OAAOA,GAAE,MAAO,iBAAgB;AAChC,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,SAAQ,QAAS,YAAW;AAErC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,CAAC;IACvB,MAAMK,MAAK,GAAIJ,SAAS,CAAC;IACzB,MAAMK,IAAG,GAAI;MAAEC,KAAK,EAAE,EAAE;MAAEC,QAAQ,EAAE;IAAG;IACvC,MAAMC,KAAI,GAAI,EAAC;IAEf,MAAMC,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC9B,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMZ,GAAG,CAACa,IAAI,CAAC,cAAc,EAAEN,IAAI;QACpDF,KAAK,CAACS,MAAM,CAAC,UAAU,EAAEF,QAAQ,CAACG,IAAI,CAACC,KAAK;QAC5CV,MAAM,CAACW,IAAI,CAAC,GAAG;MACjB,EAAE,OAAOC,GAAG,EAAE;QACZ,IAAI,CAACR,KAAI,GAAI,eAAc;MAC7B;IACF;IAEA,OAAO;MAAEH,IAAI;MAAEG,KAAK;MAAEC;IAAY;EACpC;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}